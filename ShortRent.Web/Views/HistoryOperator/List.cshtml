
@{
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}
@section Styles{
    @Styles.Render("~/Styles/BootstraTable")
}
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <div class="col-xs-12 marTOrd-xs">
                    <div class="row">
                        <label class="control-label col-sm-2 text-right" for="txt_search_pName"><span>操作人</span></label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txt_search_pName">
                        </div>
                        <label class="control-label col-sm-2 text-right" for="txt_search_Entity"><span>模块名称</span></label>
                        <div class="col-sm-4 ">
                            <input type="text" class="form-control" id="txt_search_Entity">
                        </div>
                        <div class="col-sm-12  marTop-Search text-right">
                            <button type="button" id="btn_query" class="btn btn-primary btn-sm">查询</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="box-body table-responsive">
                <table id="HistoryTable" class="table col-xs-12 col-sm-12 col-lg-12 text-nowrap"></table>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    @Scripts.Render("~/Scripts/BootstrapTable")
    @Scripts.Render("~/Scripts/BootstrapTable_zh")
    @Scripts.Render("~/Scripts/HistoryIndex")
}